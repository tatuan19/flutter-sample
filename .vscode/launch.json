	"version": "0.2.0",
	"configurations": [
		{
			// flutter run --debug --dart-define=env=.env.development
			"name": "sample",
			"request": "launch",
			"type": "dart",
			"args": [
				"--dart-define",
				"env=.env.development"
			]
		},
		{
			"name": "sample (profile mode)",
			"request": "launch",
			"type": "dart",
			"flutterMode": "profile"
		},
		{
			// flutter run --debug --dart-define=env=.env.production
			"name": "sample (release mode)",
			"request": "launch",
			"type": "dart",
			"flutterMode": "release",
			"args": [
				"--dart-define",
				"env=.env.production"
			]
		}
	]
}
